  <system.diagnostics>
    <switches>
      <!-- Switches in this section control various diagnostic messages. 
                 In order to receive data trace messages, change value="0" to a number between 1 and 4 -->
      <!-- Traces all general events in DTA Shell application -->
      <add name="Shell" value="0" />
      <!-- Traces events in SQL queries queue -->
      <add name="QueryQueue" value="0" />
      <!-- Traces events in SessionMonitor window-->
      <add name="SessionMonitor" value="0" />
      <!-- Traces events related to custom control painting -->
      <add name="CustomPainting" value="0" />
      <!-- Traces events related to command routing -->
      <add name="Commands" value="0" />
      <!-- Trace events related to Session Form window -->
      <add name="SessionForm" value="0" />
      <!-- Trace events related to remoting interface -->
      <add name="Remoting" value="0" />
      <!-- Trace events related to session progress monitor -->
      <add name="SessionProgressMonitor" value="0" />
    </switches>
  </system.diagnostics>
  <appSettings>
    <!-- This will ignore specific DTAEngine initialization failure code -->
    <add key="IgnoreInitMsdbProcessError" value="128" />
  </appSettings>
